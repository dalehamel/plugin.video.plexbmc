		<setting id="ep_view_0" type="labelenum" label="Episode View" values="None|List|MediaInfo|MediaInfo 2|Icons|Wide|Big Icons|Icons 2|Panel|Fanart|Fanart 2" default="None" subsetting="true"  visible="eq(-4,0)" enable="eq(-5,true)+eq(-4,0)"/>
		<setting id="mu_view_0" type="labelenum" label="Music View" values="None|List|MediaInfo|MediaInfo 2|Icons|Wide|Big Icons|Icons 2|Panel|Fanart|Fanart 2" default="None" subsetting="true"  visible="eq(-5,0)" enable="eq(-6,true)+eq(-5,0)"/>
		#Quartz V3 Skin
		<setting id="mo_view_1" type="labelenum" label="Movie View" values="None|List|Big List|MediaInfo|MediaInfo 2|Big Icons|Icons|Panel|Wide|Fanart 1|Fanart 2|Fanart 3" default="None" subsetting="true"  visible="eq(-7,true)+eq(-6,1)" enable="eq(-7,true)+eq(-6,1)"/>
		<setting id="tv_view_1" type="labelenum" label="TV View" values="None|List|Big List|MediaInfo|MediaInfo 2|Big Icons|Icons|Panel|Wide|Fanart 1|Fanart 2|Fanart 3" default="None" subsetting="true"  visible="eq(-8,true)+eq(-7,1)" enable="eq(-8,true)+eq(-7,1)"/>
		<setting id="se_view_1" type="labelenum" label="Season View" values="None|List|MediaInfo|MediaInfo 2|Icons|Wide|Big Icons|Icons 2|Panel|Fanart 1|Fanart 2" default="None" subsetting="true"  visible="eq(-9,true)+eq(-8,1)" enable="eq(-9,true)+eq(-8,1)"/>
		<setting id="ep_view_1" type="labelenum" label="Episode View" values="None|List|Big List|MediaInfo|MediaInfo 2|Icons|Panel|Wide|Fanart 3" default="None" subsetting="true"  visible="eq(-10,true)+eq(-9,1)" enable="eq(-10,true)+eq(-9,1)"/>
		<setting id="mu_view_1" type="labelenum" label="Music View" values="None|List|Icons|Fanart 3" default="None" subsetting="true"  visible="eq(-11,true)+eq(-10,1)" enable="eq(-11,true)+eq(-10,1)"/>
		#Confluence Skin
		<setting id="mo_view_2" type="labelenum" label="Movie View" values="None|List|Big List|Thumbnail|Poster Wrap|Fanart|Media Info|Media Info 2" default="None" subsetting="true"  visible="eq(-12,true)+eq(-11,2)" enable="eq(-12,true)+eq(-11,2)"/>
		<setting id="tv_view_2" type="labelenum" label="TV View" values="None|Big List|Thumbnail|Media Info|Media Info 2|Wide Icons" default="None" subsetting="true"  visible="eq(-13,true)+eq(-12,2)" enable="eq(-13,true)+eq(-12,2)"/>
		<setting id="se_view_2" type="labelenum" label="Season View" values="None|List|Big List|Thumbnail|Poster Wrap|Fanart|Media Info|Media Info 2" default="None" subsetting="true"  visible="eq(-14,true)+eq(-13,2)" enable="eq(-14,true)+eq(-13,2)"/>
		<setting id="ep_view_2" type="labelenum" label="Episode View" values="None|List|Big List|Thumbnail|Media Info|Media Info 2" default="None" subsetting="true"  visible="eq(-15,true)+eq(-14,2)" enable="eq(-15,true)+eq(-14,2)"/>
		<setting id="mu_view_2" type="labelenum" label="Music View" values="None|List|Big List|Thumbnail|Media Info" default="None" subsetting="true"  visible="eq(-16,true)+eq(-15,2)" enable="eq(-16,true)+eq(-15,2)"/>
    </category>

    <category label="Shelf" >
		<setting id="movieShelf" type="bool" label="Enable Movie Shelf" default="true" />
		<setting id="tvShelf" type="bool" label="Enable TV Shelf" default="true" />
		<setting id="musicShelf" type="bool" label="Enable Music Shelf" default="true" />
     	<setting id="channelShelf" type="bool" label="Enable Channel Shelf" default="true" />
		<setting id="contentFilter" type="labelenum" label="Content filter" values="Kids|Teens|Adults" default="Adults" />
		<setting id="contentNone" type="labelenum" label="Map unknown content as:" values="Kids|Teens|Adults" default="Adults" />
	</category>

    <category label="Debug" >
        <setting id="debug" type="bool" label="30014" default="false" />
        <setting id="libraryfilter" type="text" label="RA Library filter" default="" />
    </category>

</settings>
